{
    "name": "simone-bianco/laravel-patches",
    "version": "1.0.0",
    "description": "Laravel package to manage patches for your application, like database migrations but for code.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "Simone Bianco"}
    ],
    "require": {
        "php": ">=8.3",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/cache": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "orchestra/testbench": "^10.6",
        "spatie/laravel-package-tools": "^1.92"
    },
    "autoload": {
        "psr-4": {
            "SimoneBianco\\Patches\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimoneBianco\\Patches\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimoneBianco\\Patches\\PatchesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
